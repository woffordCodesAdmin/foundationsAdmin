<p id="notice"><%= notice %></p>

<h1>Daily Vids</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Frame url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daily_vids.each do |daily_vid| %>
      <tr>
        <td><%= daily_vid.title %></td>
        <td><%= daily_vid.description %></td>
        <td><%= daily_vid.frame_url %></td>
        <td><%= link_to 'Show', daily_vid %></td>
        <td><%= link_to 'Edit', edit_daily_vid_path(daily_vid) %></td>
        <td><%= link_to 'Destroy', daily_vid, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Daily Vid', new_daily_vid_path %>
